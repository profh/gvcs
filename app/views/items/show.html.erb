<% if !current_user.nil? && current_user.role?(:case_worker) %>
    <%= render partial: "partials/show_structure", locals: {model_name: "item", 
                                                            object: @item,
                                                            primary: "file_data", 
                                                            secondary: nil,
                                                            sidebar: nil} %>
<% elsif !current_user.nil? && current_user.role?(:parent) %>
    <%= render partial: "partials/show_structure", locals: {model_name: "item", 
                                                            object: @item,
                                                            primary: "file_data", 
                                                            secondary: "completion_status",
                                                            sidebar: "recent_submissions"} %>
<% end %>