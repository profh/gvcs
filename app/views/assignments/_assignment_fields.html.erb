<%= f.input :item_id, collection: Item.active.alphabetical, include_blank: false %>
<%= f.input :parent_id, collection: Parent.active.alphabetical.map{|p| ["#{p.p1_last_name}, #{p.p1_first_name}", p.id]}, include_blank: false %>
<%= f.input :due_date, as: :date, include_blank: false %>
<%= f.hidden_field :completion, :value => false %>
