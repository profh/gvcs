<% if !current_user.nil? && current_user.role?(:case_worker) %>
    <%= render partial: "partials/show_structure", locals: {model_name: "category", 
                                                            object: @category,
                                                            primary: "item_data", 
                                                            secondary: nil,
                                                            sidebar: "category_data"} %>
<% elsif !current_user.nil? && current_user.role?(:parent) %>
    <%= render partial: "partials/show_structure", locals: {model_name: "category", 
                                                            object: @category,
                                                            primary: "item_by_assignment_data", 
                                                            secondary: nil,
                                                            sidebar: nil} %>
<% end %>